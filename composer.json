{
  "name": "alexlcdee/messenger-tarantool",
  "description": "Symfony Messenger Tarantool Transport",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {"name": "alexlcdee", "email": "info@alexlcdee.ru"}
  ],
  "require": {
    "php": "^7.4",
    "psr/log": "^1.1",
    "symfony/messenger": "^5.0",
    "tarantool/queue": "^0.8.0"
  },
  "autoload": {
    "psr-4": {"AlexLcDee\\Messenger\\Tarantool\\": "src/"},
    "exclude-from-classmap": ["tests/"]
  },
  "autoload-dev": {
    "psr-4": {"AlexLcDee\\Messenger\\Tarantool\\Tests\\": "tests/"}
  },
  "require-dev": {
    "rybakit/msgpack": "^0.7.0",
    "phpunit/phpunit": "^9.1",
    "tarantool/client": "^0.7.0",
    "tarantool/stubs": "dev-master",
    "ext-json": "*"
  },
  "suggest": {
    "ext-tarantool": "For using tarantool pecl extension",
    "tarantool/client": "For using pure tarantool client"
  }
}
